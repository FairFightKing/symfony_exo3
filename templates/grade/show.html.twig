{% extends 'base.html.twig' %}

{% block title %}Grade{% endblock %}

{% block body %}
    <h1>Grade</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ grade.id }}</td>
            </tr>
            <tr>
                <th>Grade</th>
                <td>{{ grade.grade }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ grade.date ? grade.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('grade_index') }}">back to list</a>

    <a href="{{ path('grade_edit', {'id': grade.id}) }}">edit</a>

    {{ include('grade/_delete_form.html.twig') }}
{% endblock %}
